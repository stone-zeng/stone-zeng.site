{
  "compilerOptions": {
    "baseUrl": "./",
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "module": "esnext",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "target": "esnext",
    "types": [
      "vitepress/client",
      "node"
    ],
  },
  "include": [
    ".vitepress/*.ts",
    ".vitepress/theme/**/*",
  ]
}