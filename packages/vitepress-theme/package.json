  "dependencies": {
    "@giscus/vue": "^3.0.0",
    "katex": "^0.16.15",
    "vitepress": "1.5.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@tabler/icons-vue": "^3.24.0",
    "@types/katex": "^0.16.7",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "autoprefixer": "^10.4.20",
    "medium-zoom": "^1.1.0",
    "tailwindcss": "^3.4.16",
    "vite": "^6.0.3",
    "vite-plugin-dts": "4.3.0"
  }
