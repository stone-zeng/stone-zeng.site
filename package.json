    "katex": "^0.16.9",
    "markdown-it-attrs": "^4.1.6",
    "markdown-it-multimd-table": "^4.2.3",
    "markdown-it-plugins": "workspace:^",
    "ora": "^7.0.1",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "shikiji": "^0.7.5",
    "tailwindcss": "^3.3.5",
    "vite": "^5.0.4",
    "vitepress": "1.0.0-rc.31",
