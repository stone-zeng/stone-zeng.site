  PlotRange  -> All,
  PlotTheme  -> {"Detailed", "ThickLines"},
  PlotStyle  -> "RoseColors",
  LabelStyle -> {FontFamily -> "Roboto"},
  ImageSize  -> 340,
  FilterRules[{opts}, Options @ ListPlot]]
